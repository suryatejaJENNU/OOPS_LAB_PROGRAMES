class Car{
	String company;
	double mileage;
	double Speed;
	String color;
	Car(String company,double mileage,double Speed,String color) {
		this.company = company;
		this.mileage = mileage;
		this.Speed = Speed;
		this.color = color;
	}
}

class Cars {
	public static void main(String args[]) {
		Car c1 = new Car("roles roy",66.9,120.2,"grey");
	}
}
